<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="True">
    <uie:ObjectField label="Project" name="project-object" type="Lungfetcher.Editor.Scriptables.ProjectSo, Lungfecther.Editor" binding-path="project" style="position: relative; flex-wrap: wrap; height: 18px;" />
    <ui:DropdownField label="Project Container" name="containers-dropdown" index="-1" />
    <ui:EnumField label="Strategy" binding-path="strategy" type="Lungfetcher.Editor.Scriptables.ContainerSo+ContainerStrategy, Lungfecther.Editor" name="strategy-enum" />
    <uie:ObjectField label="Table Collection" name="table-reference-object" binding-path="stringTableCollection" type="UnityEditor.Localization.StringTableCollection, Unity.Localization.Editor" />
    <ui:Button text="Sync" parse-escape-sequences="true" display-tooltip-when-elided="true" name="sync-container-btn" />
    <ui:Button text="Hard Sync" parse-escape-sequences="true" display-tooltip-when-elided="true" name="hard-sync-container-btn" />
    <ui:ScrollView name="progress-view" />
    <ui:Label tabindex="-1" text="Last Synced at:" parse-escape-sequences="true" display-tooltip-when-elided="true" name="updated-label" />
</ui:UXML>
